generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model Deliveryman {
  id       String @id @default(uuid())
  username String @unique
  password String

  @@map("deliverymans")
}

model Client {
  id       String @id @default(uuid())
  username String @unique
  password String

  @@map("clients")
}
